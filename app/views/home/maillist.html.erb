			it2.style.display="none";
			imgs[0].src="http://my.snu.ac.kr/mysnu/Html2/images/fac/tabtiltle01_on_en.gif";
			imgs[1].src="http://my.snu.ac.kr/mysnu/Html2/images/fac/tabtiltle03_off_en.gif";
		}else if(e=='2'){
			it1.style.display="none";
			it2.style.display="block";
			imgs[0].src="http://my.snu.ac.kr/mysnu/Html2/images/fac/tabtiltle01_off_en.gif";
			imgs[1].src="http://my.snu.ac.kr/mysnu/Html2/images/fac/tabtiltle03_on_en.gif"; 
		} 
	}

