 <script language="JavaScript">
	var helpPop = null;
	var flagHelp = false;
	var topMargin,leftMargin;       
	function updatePop(e) {
		x = (document.all) ? event.clientX + document.body.scrollLeft : e.pageX;
		y = (document.all) ? event.clientY + document.body.scrollTop  : e.pageY;
		if (flagHelp != false){
			helpPop = document.getElementById('helpLayer');
			helpPop.style.left = (x + leftMargin) + "px";
			helpPop.style.top  = (y + topMargin) + "px";
			helpPop.style.display = "block";
		}
	}
	function popUp(langKnd,width,top,left) {
		var msg ="";
		if(langKnd == "ko"){
			msg = "메인화면의 우측 [행사/이벤트] 또는 정보광장 > 스누이벤트에서 마음에 드는 행사/이벤트의 [I'M GOING] 버튼을 누르면 관심일정에 등록됩니다.";
		}else if(langKnd == "en"){
			msg = "Click [Event] in the right side of the front page or click [SNU Events] on the [Information Plaza] menu bar. If you click on the [I’M GOING] button, that event will be registered in your schedule.";
		}
		topMargin = top;
		leftMargin = left;
		helpPop = document.getElementById('helpLayer');
		 var content = "<table width="+width+" border=1 cellspacing=0 cellpadding=0><tr>"+
		"<td style='padding:10px 10px 10px 10px; line-height:18px; word-break:break-all' bgcolor=#F6F4F4>"+msg+"</td></tr></table>";
		helpPop.innerHTML = content;
		flagHelp = true;
		document.onmousemove = updatePop;
	}
	function popClose() {
		helpPop.style.display = "none";
		flagHelp = false;
	}

 
	function tabDisplay(e){
		
		var it1 = document.getElementById("item1");
		var it2 = document.getElementById("item2");
		var imgs = document.getElementsByTagName("img");
		
		if(e=='1'){
